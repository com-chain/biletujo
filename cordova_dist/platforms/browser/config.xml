<?xml version='1.0' encoding='utf-8'?>
<widget id="org.comchain.wallet" version="2.3.7" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Biletujo</name>
    <description>
        An Apache Cordova wallet for Com-Chain
    </description>
    <author email="it@monnaie-leman.org" href="http://com-chain.org">
        Biletujo
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="AndroidLaunchMode" value="singleInstance" />
</widget>
